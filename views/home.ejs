                shape="poly"
            />
            <area
                alt="drawing"
                title="drawing"
                href="/portal/drawing"
                coords="38,58,64,101"
                shape="rect"
            />
            <area
                alt="food"
                title="food"
                href="/portal/food"
                coords="338,225,320,233,326,252,342,255,350,235"
                shape="poly"
            />
            <area
                alt="plants"
                title="plants"
                href="/portal/plants"
                coords="250,144,212,180,214,226,260,233,279,177"
                shape="poly"
            />
            <area
                alt="tea"
                title="tea"
                href="/portal/tea"
                coords="274,255,278,275,295,274,299,251,298,223,278,217"
                shape="poly"
            />
            <area
                alt="bed"
                title="bed"
                href="/portal/bed"
                coords="491,201,456,277,440,355,638,306,638,186"
                shape="poly"
            />
        </map>
    </body>
    <script src="/js/script.js"></script>
    <script src="/js/toggle.js"></script>
    <script
        src="https://kit.fontawesome.com/3cf526719d.js"
        crossorigin="anonymous"
    ></script>
</html>
